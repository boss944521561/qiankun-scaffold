{
  "name": "performance-monitoring-system",
  "version": "1.0.0",
  "description": "前端PMS-性能监控系统",
  "author": "Mason.meng",
  "scripts": {
    "↓↓↓↓↓↓↓↓↓↓↓↓ 全部启动 ↓↓↓↓↓↓↓↓↓↓↓↓": "————————————————彩蛋：————————————————————",
    "dev": "npm-run-all --parallel dev:*",
    "build": "npm-run-all --serial build:*",
    "i": "yarn un && yarn in",
    "in": "yarn add npm-run-all --dev && npm-run-all --serial in:*",
    "un": "yarn add npm-run-all --dev && npm-run-all --serial un:*",
    "↓↓↓↓↓↓↓↓↓↓↓↓ 逐一启动 ↓↓↓↓↓↓↓↓↓↓↓↓": "————————————————老头好，老头有低保！————————————————",
    "dev:main": "cd src/main && yarn start",
    "dev:react": "cd src/react && yarn start",
    "dev:vue": "cd src/vue && yarn start",
    "build:main": "cd src/main && yarn build",
    "build:react": "cd src/react && yarn build",
    "build:vue": "cd src/vue && yarn build",
    "in:pms": "yarn",
    "in:main": "cd src/main && yarn",
    "in:react": "cd src/react && yarn",
    "in:vue": "cd src/vue && yarn",
    "un:main": "cd src/main && rm -rf node_modules/",
    "un:react": "cd src/react && rm -rf node_modules/",
    "un:vue": "cd src/vue && rm -rf node_modules/",
    "un:pms": "rm -rf node_modules/"
  },
  "license": "MIT",
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
